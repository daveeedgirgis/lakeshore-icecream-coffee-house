{
  "permissions": {
    "allow": [
      "Bash(git init:*)",
      "Bash(npm create:*)",
      "Bash(mkdir:*)",
      "Bash(npm install)",
      "Bash(npm run build:*)",
      "Bash(pkill:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(env)",
      "Bash(gh auth:*)",
      "Bash(gh repo create:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(echo $SHELL)",
      "Bash(export RENDER_API_KEY:*)",
      "Bash(npm install:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:render.com)",
      "WebFetch(domain:themewagon.github.io)",
      "WebFetch(domain:www.google.com)",
      "WebFetch(domain:maps.google.com)",
      "Bash(cp:*)",
      "Bash(mv:*)"
    ],
    "deny": []
  },
  "mcpServers": {
    "render": {
      "url": "https://mcp.render.com/mcp",
      "headers": {
        "Authorization": "Bearer ${RENDER_API_KEY}"
      }
    }
  }
}